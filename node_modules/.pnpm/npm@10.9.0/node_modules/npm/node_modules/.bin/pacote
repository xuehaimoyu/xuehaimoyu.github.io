#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules/npm/node_modules/pacote/bin/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules/npm/node_modules/pacote/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules/npm/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules/npm/node_modules/pacote/bin/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules/npm/node_modules/pacote/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules/npm/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/npm@10.9.0/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pacote/bin/index.js" "$@"
else
  exec node  "$basedir/../pacote/bin/index.js" "$@"
fi
