#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/marked@14.1.3/node_modules/marked/bin/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/marked@14.1.3/node_modules/marked/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/marked@14.1.3/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/marked@14.1.3/node_modules/marked/bin/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/marked@14.1.3/node_modules/marked/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/marked@14.1.3/node_modules:/mnt/d/zhangsiranwork/viteblog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/marked.js" "$@"
else
  exec node  "$basedir/../../bin/marked.js" "$@"
fi
